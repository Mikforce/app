<h2>mrt</h2>

<%= form_with model: @tamplate do |form| %>
<p><%= form.label :name, 'Имя' %></p>
<p><%= form.text_area :name, rows: 1 %></p>
<p><%= form.label :chernovik, 'Чероновик' %></p>
<p><%= form.text_area :chernovik, rows: 1 %></p>
<p><%= form.label :opisanie, 'Описание' %></p>
<p><%= form.text_area :opisanie, rows: 1 %></p>
<p><%= form.submit 'Создать' %></p>

<% end %>
